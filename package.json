{
    "name": "destressing-drops",
    "version": "0.0.2",
    "scripts": {
        "watch": "watchify -vd -p browserify-reload -e resources/app.js -o www/build/app.js",
        "build": "browserify resources/app.js -o www/build/app.js",
        "serve": "http-server ./www"
    },
    "browserify": {
        "transform": [
            "babelify"
        ]
    },
    "dependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "babelify": "^7.3.0",
        "browserify": "^14.4.0",
        "browserify-hmr": "^0.3.5",
        "browserify-reload": "^1.1.1",
        "http-server": "^0.10.0",
        "lodash": "^4.17.4",
        "three": "^0.87.1",
        "watchify": "^3.9.0"
    }
}
